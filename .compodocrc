{
  "$schema": "./node_modules/@compodoc/compodoc/src/config/schema.json",
  "customFavicon": "./apps/showcase-ui/src/favicon.ico",
  "customLogo": "./tools/mark-small.svg",
  "disableDomTree": true,
  "disableInternal": true,
  "disableLifeCycleHooks": true,
  "disablePrivate": true,
  "disableRoutesGraph": true,
  "disableStyleTab": true,
  "disableTemplateTab": true,
  "disableCoverage": true,
  "hideGenerator": true,
  "name": "Terminus UI",
  "navTabConfig": [
    {
      "id": "info",
      "label": "API"
    },
    {
      "id": "source",
      "label": "TypeScript"
    },
    {
      "id": "example",
      "label": "Live Demo"
    }
  ],
  "output": "./docs",
  "theme": "laravel",
  "tsconfig": "./libs/tsconfig.compodoc.json"
}
